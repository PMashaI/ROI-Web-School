@model Crm.Admin.Models.UserListModel
@{
    ViewBag.Title = "CRM Admin";
}
<h1>@ViewBag.Title</h1>
@using (Ajax.BeginForm(
    "UserList",
    new AjaxOptions
    {
        HttpMethod = "post",
        UpdateTargetId = "user-list",
        InsertionMode = InsertionMode.Replace
    }
))
{
    @Html.RenderPartial("UserList", Model.Users)
}